<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- 加载滚动条样式 -->
    <style name="loading">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/progress_bar_bg</item>
        <item name="android:indeterminateDuration">1200</item>
        <item name="android:indeterminateOnly">true</item>
    </style>
    <!-- 进度-->
    <style name="AVLoadingIndicatorView">
        <item name="minWidth">96dip</item>
        <item name="maxWidth">168dip</item>
        <item name="minHeight">96dip</item>
        <item name="maxHeight">168dip</item>
        <item name="indicatorName">BallPulseIndicator</item>
    </style>

    <style name="AppTheme.TransparentStausBar" parent="AppTheme"></style>

